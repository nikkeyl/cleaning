{
    "extends": [
        "plugin:prettier/recommended",
        "eslint:recommended"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ignorePatterns": [
        "map.js"
    ],
    "rules": {
        "no-inner-declarations": 0,
        "no-useless-escape": 0,
        "no-console": 0
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}