                "row-gap",
                "grid-row-start",
                "gap",
                "grid-template",
                "grid-template-areas",
                "grid-template-columns",
                "grid-template-rows",
                "list-style",
                "list-style-type",
                "list-style-position",
                "list-style-image",
                "caption-side",
                "empty-cells",
                "table-layout",
                "vertical-align",
                "clip-path",
                "mask",
                "mask-clip",
                "mask-composite",
                "mask-image",
                "mask-mode",
                "mask-origin",
                "mask-position",
                "mask-repeat",
                "mask-size",
