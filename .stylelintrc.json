                "border-image-width",
                "border-image-outset",
                "border-image-repeat",
                "border-radius",
                "border-top-left-radius",
                "border-top-right-radius",
                "border-bottom-right-radius",
                "border-bottom-left-radius",
                "border-spacing",
                "padding",
                "padding-top",
                "padding-right",
