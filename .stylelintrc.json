                "font-weight",
                "font-style",
                "font-display",
                "font-feature-settings",
                "font-kerning",
                "font-stretch",
                "font-synthesis",
                "font-variant",
                "font-variant-alternates",
                "font-variant-caps",
                "font-variant-east-asian",
                "font-variant-ligatures",
                "font-variant-numeric",
                "font-variant-position",
                "font-size",
                "font-size-adjust",
                "unicode-bidi",
                "unicode-range",
                "line-break",
                "line-height",
                "letter-spacing",
                "word-break",
                "word-spacing",
                "word-wrap",
